/*font*/
@font-face {
font-family: Optima;
src: url('<%= font_path 'optima regular.eot' %>') format('embedded-opentype'), /* IE6-IE8 */
    url('<%= font_path 'optima regular.ttf' %>') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('<%= font_path 'optima regular.woff' %>') format('woff'),
    url('<%= font_path 'optima regular.svg' %>') format('svg');
}
