/*font*/
@font-face {
  font-family: Optima;
  src: url('<%= font_path 'optima regular.eot' %>') format('embedded-opentype'),
  url('<%= font_path 'optima regular.ttf' %>') format('truetype'),
  url('<%= font_path 'optima regular.woff' %>') format('woff'),
  url('<%= font_path 'optima regular.svg' %>') format('svg');
}
